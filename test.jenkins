#!groovy

pipeline {
    agent any
    options([disableConcurrentBuilds(), pipelineTriggers([pollSCM('H/15 * * * *')]), timestapse()])
    stages {
        stage("Simple test 8") {
            steps {
                bat 'cd'
            }
        }
    }
    post {
        always {
            githubStatus context: 'continuous-integration/jenkins', state: 'success'
            if (env.CHANGE_ID) {
                githubComment message: "The pipeline completed successfully!"
                githubLabel labels: ['approved']
            }
            cleanWs()
        }
    }
}
